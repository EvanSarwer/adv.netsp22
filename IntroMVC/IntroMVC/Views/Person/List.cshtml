@model List<IntroMVC.Models.Person>
@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_loggedin.cshtml";
}

<h2>List</h2>

<table class="table table-bordered">
    <tr>
        <th>Name</th>
        <th>Id</th>
        <th>Dob</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Id</td>
            <td>@item.Dob</td>
        </tr>

    }
    
</table>